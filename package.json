{
  "name": "@capybaracode/awslocal",
  "version": "1.2.2",
  "description": "awslocal is a simple AWS Lambda runtime emulator aimed at providing faster development...",
  "main": "./build/cli.js",
  "types": "./build/cli.d.ts",
  "type": "module",
  "preferGlobal": true,
  "bin": {
    "awslocal": "./build/cli.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage",
    "prebuild": "rm -rf build && rm -rf dist",
    "build": "tsc -p .",
    "postbuild": "mv dist/src build && rm -rf dist",
    "local": "sudo npm i -g && awslocal",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miqueiasbrs/awslocal.git"
  },
  "keywords": [
    "aws",
    "cli",
    "local",
    "lambda",
    "runtime",
    "emulator",
    "serverless"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "miqueiasbrs <miqueias@capybaracode.com.br> (https://capybaracode.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miqueiasbrs/awslocal/issues"
  },
  "homepage": "https://github.com/miqueiasbrs/awslocal#readme",
  "devDependencies": {
    "@types/jest": "29.5.8",
    "@types/node": "20.9.0",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.7.0",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "3.449.0",
    "@aws-sdk/client-lambda": "3.449.0",
    "commander": "11.1.0",
    "dotenv": "16.3.1",
    "path-to-regexp": "6.2.1",
    "uuid": "9.0.1",
    "winston": "3.11.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    "#*": [
      "./build/*"
    ]
  }
}
